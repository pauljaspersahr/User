{
    "dev.containers.executeInWSL": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<esc>"
            ]
        },
        {
            "before": [
                "<ctrl>",
                "<Space>"
            ],
            "commands": [
                "editor.action.triggerSuggest"
            ],
            "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
        }
    ],
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Space>",
                "s",
                "h"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "v"
            ],
            "commands": [
                "workbench.action.moveEditorToNextGroup"
            ]
        },
        {
            "before": [
                "<space>",
                "s",
                "c"
            ],
            "commands": [
                "workbench.action.moveEditorToPreviousGroup"
            ]
        },
        {
            "before": [
                "<space>",
                "s",
                "s"
            ],
            "commands": [
                "workbench.action.newGroupRight",
                "workbench.action.focusPreviousGroup",
                "workbench.action.moveEditorToNextGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "h"
            ],
            "commands": [
                "workbench.action.moveEditorToLeftGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "j"
            ],
            "commands": [
                "workbench.action.moveEditorToBelowGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "k"
            ],
            "commands": [
                "workbench.action.moveEditorToAboveGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "l"
            ],
            "commands": [
                "workbench.action.moveEditorToRightGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "x"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<Space>",
                "q",
                "q"
            ],
            "command": [
                "workbench.action.closeEditorsAndGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "e"
            ],
            "commands": [
                "workbench.action.evenEditorWidths"
            ]
        },
        {
            "before": [
                "<Space>",
                "s",
                "m"
            ],
            "commands": [
                "workbench.action.toggleMaximizeEditorGroup"
            ],
            "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
        },
        {
            "before": [
                "<Space>",
                "s",
                "w"
            ],
            "commands": [
                "workbench.action.closeEditorsInGroup"
            ]
        },
        {
            "before": [
                "<Space>",
                "f",
                "m"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<esc>",
                "<esc>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<Space>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<Tab>",
                "<Tab>"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
            ],
            "when": "vim.mode == 'Normal' && !activeEditorGroupEmpty"
        },
        {
            "before": [
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ],
            "when": "editorHasDefinitionProvider && editorTextFocus"
        },
        {
            "before": [
                "g",
                "D"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
        {
            "before": [
                "<Space>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<Space>",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-n>": false
    },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "editor.minimap.enabled": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "cmake.configureOnOpen": true,
    "cmake.options.statusBarVisibility": "compact",
    "cmake.showOptionsMovedNotification": false,
    "editor.inlayHints.padding": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": true,
    "editor.lineDecorationsWidth": 5,
    "editor.padding.top": 10,
    "editor.folding": false,
    "editor.glyphMargin": true,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "zenMode.hideLineNumbers": false,
    "search.showLineNumbers": true,
    "zenMode.showTabs": "single",
    "workbench.editor.empty.hint": "hidden",
    "explorer.confirmDelete": false,
    "remote.autoForwardPortsSource": "hybrid",
    "editor.formatOnSave": true,
    "gitlens.currentLine.scrollable": false,
    "gitlens.codeLens.enabled": false,
    "editor.codeLens": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.colorCustomizations": {
        "terminalCursor.background": "#ABB2BF",
        "terminalCursor.foreground": "#ABB2BF",
        "terminal.ansiBlack": "#282C34",
        "terminal.ansiBlue": "#61AFEF",
        "terminal.ansiBrightBlack": "#545862",
        "terminal.ansiBrightBlue": "#61AFEF",
        "terminal.ansiBrightCyan": "#56B6C2",
        "terminal.ansiBrightGreen": "#98C379",
        "terminal.ansiBrightMagenta": "#C678DD",
        "terminal.ansiBrightRed": "#E06C75",
        "terminal.ansiBrightWhite": "#C8CCD4",
        "terminal.ansiBrightYellow": "#E5C07B",
        "terminal.ansiCyan": "#56B6C2",
        "terminal.ansiGreen": "#98C379",
        "terminal.ansiMagenta": "#C678DD",
        "terminal.ansiRed": "#E06C75",
        "terminal.ansiWhite": "#ABB2BF",
        "terminal.ansiYellow": "#E5C07B"
    },
    "zenMode.fullScreen": false,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "debug.onTaskErrors": "abort",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vim.highlightedyank.enable": true,
    "vim.foldfix": true,
    "vim.highlightedyank.duration": 400,
    "gitlens.views.workspaces.showIncomingActivity": true,
    "files.exclude": {
        "**/__pycache__": true
    },
    "vim.smartRelativeLine": true,
    "editor.lineNumbers": "relative",
    "editor.fontFamily": "'MesloLGL Nerd Font Mono Regular'",
    "vim.highlightedyank.color": "rgba(250, 250, 170, 0.7)",
    "autopep8.path": [
        "/usr/local/bin/autopep8"
    ],
    "github.copilot.editor.enableAutoCompletions": true,
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "zenMode.hideStatusBar": false,
    "window.commandCenter": false,
    "eslint.useFlatConfig": true,
    "zenMode.centerLayout": false,
    "window.title": "${dirty}${remoteName}${separator}${rootNameShort}${separator}${activeEditorShort}${separator}${profileName}${separator}${appName}",
    "window.menuBarVisibility": "compact",
    "workbench.sideBar.location": "right",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "workbench.activityBar.orientation": "vertical",
}
